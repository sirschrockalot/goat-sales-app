{
  "name": "goat-sales-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:local": "NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "db:seed": "tsx scripts/seed-local.ts",
    "db:seed:production": "tsx scripts/seed-production.ts",
    "evolve:prompts": "tsx scripts/evolve-prompts.ts",
    "db:sync": "tsx scripts/sync-schema-from-production.ts",
    "db:reset": "supabase db reset && npm run db:seed",
    "dna:scrape": "tsx scripts/dnaScraper.ts",
    "dna:analyze": "tsx scripts/dnaScraper.ts analyze",
    "dna:batch": "tsx scripts/batchDnaScraper.ts",
    "prompt:merge": "tsx scripts/promptMerger.ts",
    "apex:merge": "tsx scripts/apexPromptMerger.ts",
    "portal:scrape": "tsx scripts/privatePortalScraper.ts",
    "portal:scrape:url": "tsx scripts/privatePortalScraper.ts",
    "videos:transcribe": "tsx scripts/privateVideoTranscriber.ts",
    "videos:process": "tsx scripts/privateVideoProcessor.ts",
    "scrape-dna": "tsx scripts/batchDnaScraper.ts",
    "scrape-portal": "tsx scripts/privatePortalScraper.ts",
    "transcribe-private": "tsx scripts/privateVideoProcessor.ts",
    "merge-prompts": "tsx scripts/apexPromptMerger.ts",
    "final-check": "tsx scripts/finalCheck.ts",
    "test:ai-services": "tsx scripts/test-ai-services.ts",
    "archive:calls": "tsx scripts/archive-calls.ts",
    "audit:knowledge": "tsx scripts/knowledgeAudit.ts",
    "audit:knowledge:protect": "tsx scripts/knowledgeAudit.ts --auto-protect",
    "vapi:list": "tsx scripts/list-vapi-assistants.ts",
    "vapi:cleanup": "tsx scripts/cleanup-vapi-assistants.ts --delete",
    "elevenlabs:voices": "tsx scripts/list-elevenlabs-voices.ts",
    "vapi:remove-unpublished": "tsx scripts/remove-unpublished-or-error-assistants.ts",
    "vapi:remove-unpublished:delete": "tsx scripts/remove-unpublished-or-error-assistants.ts --delete --yes",
    "apex:insights": "tsx scripts/apexInsights.ts",
    "sandbox:generate-personas": "tsx scripts/personaGenerator.ts",
    "sandbox:battle": "tsx scripts/autonomousBattle.ts",
    "sandbox:promote-tactics": "tsx scripts/promoteTactics.ts",
    "sandbox:adversarial": "tsx scripts/adversarialTrainer.ts",
    "sandbox:golden-samples": "tsx scripts/goldenSamples.ts",
    "sandbox:audit-vocal-soul": "tsx scripts/vocalSoulAuditor.ts",
    "sandbox:batch-audit-humanity": "tsx scripts/batch-audit-humanity-grades.ts",
    "scout:start": "tsx scripts/startTacticalScout.ts",
    "scout:scan": "tsx scripts/scanBreakthroughs.ts",
    "audit:daily": "tsx scripts/dailyAuditor.ts",
    "research:pre-call": "tsx scripts/preCallResearch.ts",
    "init": "chmod +x scripts/sync-env.sh && ./scripts/sync-env.sh",
    "doppler:setup": "chmod +x scripts/setup-doppler.sh && ./scripts/setup-doppler.sh",
    "doppler:secrets": "doppler secrets --config dev",
    "dev:doppler": "doppler run -- npm run dev",
    "ignite": "EXPLICIT_ENV=sandbox tsx scripts/ignite-training.ts",
    "train:trigger": "tsx scripts/trigger-training-local.ts",
    "train:direct": "EXPLICIT_ENV=sandbox tsx scripts/run-training-direct.ts",
    "train:schedule:setup": "chmod +x scripts/setup-training-schedule.sh && bash scripts/setup-training-schedule.sh",
    "train:schedule:run": "EXPLICIT_ENV=sandbox tsx scripts/scheduled-training.ts 5",
    "train:schedule:stop": "crontab -l 2>/dev/null | grep -v 'run-training-cron.sh' | crontab - && echo '✅ Training schedule stopped'",
    "train:schedule:status": "crontab -l 2>/dev/null | grep 'run-training-cron.sh' || echo '❌ No training schedule found'",
    "train:schedule:logs": "tail -f logs/cron-training.log",
    "train:test:vercel": "tsx scripts/test-training-vercel.ts",
    "seed:verify": "EXPLICIT_ENV=sandbox tsx scripts/verify-seed.ts",
    "seed:sandbox": "EXPLICIT_ENV=sandbox tsx scripts/seed-sandbox.ts",
    "seed:instructions": "tsx scripts/execute-seed-via-dashboard.ts",
    "migration:sandbox": "EXPLICIT_ENV=sandbox tsx scripts/execute-sandbox-migration.ts",
    "migration:verify": "EXPLICIT_ENV=sandbox tsx scripts/verify-migration.ts"
  },
  "dependencies": {
    "@react-email/components": "^0.0.25",
    "@supabase/supabase-js": "^2.90.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/chokidar": "^1.7.5",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@vapi-ai/web": "^2.5.2",
    "canvas-confetti": "^1.9.4",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.25.0",
    "fs-extra": "^11.3.3",
    "lucide-react": "^0.562.0",
    "next": "^15.1.0",
    "openai": "^6.16.0",
    "playwright": "^1.57.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "6.7.0",
    "slugify": "^1.6.6",
    "winston": "^3.19.0",
    "youtube-transcript": "^1.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/winston": "^2.4.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "pg": "^8.16.3",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.0"
  }
}
