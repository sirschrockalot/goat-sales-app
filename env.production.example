# Production Environment Variables
# Copy this to .env.production and fill in your production values
# Used when deploying to Vercel

# Supabase Production Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Vapi Configuration
NEXT_PUBLIC_VAPI_API_KEY=your_vapi_public_key
VAPI_SECRET_KEY=your_vapi_secret_key
ACQUISITIONS_ASSISTANT_ID=aaf338ae-b74a-43e4-ac48-73dd99817e9f

# ElevenLabs Configuration (for high-fidelity voices)
ELEVEN_LABS_API_KEY=your_elevenlabs_api_key

# Deepgram Configuration (for fast STT)
DEEPGRAM_API_KEY=your_deepgram_api_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# App Configuration
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app

# Cron Secret (generate with: openssl rand -hex 32)
CRON_SECRET=your_cron_secret_here

# Optional: Email Configuration
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=noreply@yourdomain.com

# Optional: Rate Limiting
UPSTASH_REDIS_REST_URL=your_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_token
